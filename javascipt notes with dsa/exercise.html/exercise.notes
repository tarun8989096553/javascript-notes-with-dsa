  {
     display()
    {
      document.writeln("space is invoked<br>");
    }
  }
class B extends A
  {
    display()
    {
      document.writeln("spacex is invoked");
    }
  }

var a=[new A(), new B()]
a.forEach(function(msg)
{
msg.display();
});
</script>
Output :
space is invoked
spacex is invoked
